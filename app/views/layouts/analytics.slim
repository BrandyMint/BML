doctype html
html
  head
    title= AppSettings.title
    = bugsnag_script
    == stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    == javascript_include_tag 'application', 'data-turbolinks-track' => true

    == stylesheet_link_tag    'funnel', media: 'all', 'data-turbolinks-track' => true
    == javascript_include_tag 'funnel', 'data-turbolinks-track' => true
    == csrf_meta_tags
    == include_gon
  body
    == render 'layouts/landing/topmenu'
    .page-content
      .container.sub-top-menu
        == render 'layouts/analytics/topmenu'

      .container
        == yield
