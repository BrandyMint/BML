= simple_form_for [current_landing, collection, column] do |f|
  = f.input :title, label: 'Человеческое название колонки'
  = f.input :key, label: 'Ключ (название поля из html-формы)', hint: f.object.key
  - if f.object.persisted?
    = f.input :uuid, disabled: true
  = f.submit t('shared.save'), class: 'btn btn-primary'
